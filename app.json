{
  "expo": {
    "name": "MeowMentor",
    "slug": "MeowMentor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "A macska profilképének feltöltéséhez szükséges.",
        "NSPhotoLibraryUsageDescription": "A galériából kép kiválasztásához szükséges.",
        "NSUserNotificationUsageDescription": "Értesítések az etetésről, vízcseréről és állatorvosi időpontokról."
      }
    },
    "android": {
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
